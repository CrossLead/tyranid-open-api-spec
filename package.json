{
  "name": "tyranid-openapi",
  "version": "0.1.0",
  "description": "Open API spec and express app generator for tyranid",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "test": "bash -c \"mkdir -p ./.tmp/ && rm -rf ./dist/ && npm run lint:fix && npm run build && npm run ava\"",
    "ava": "ava",
    "build": "tsc",
    "watch": "tsc -w",
    "lint:fix": "tslint --fix -c tslint.json './src/**/*.ts'",
    "lint": "tslint -c tslint.json './src/**/*.ts'",
    "prettier": "prettier --single-quote --write './src/**/*.ts'"
  },
  "keywords": ["tyranid", "swagger", "openapi", "api"],
  "author": "bsouthga@gmail.com",
  "license": "Apache-2.0",
  "dependencies": {
    "@types/swagger-schema-official": "^2.0.4",
    "ajv": "^5.0.1",
    "js-yaml": "^3.8.3",
    "swagger-schema-official": "^2.0.0-bab6bed"
  },
  "peerDependencies": {
    "tyranid": "^0.1.57"
  },
  "devDependencies": {
    "@types/js-yaml": "3.5.29",
    "@types/lodash": "4.14.62",
    "@types/node": "6.0.68",
    "ava": "0.19.1",
    "prettier": "^1.5.3",
    "tslint": "5.1.0",
    "typedoc": "^0.6.0",
    "typescript": "2.3.1",
    "tyranid": "^0.1.57"
  },
  "ava": {
    "files": ["dist/test/index.js"]
  }
}
