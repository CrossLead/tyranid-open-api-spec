  },
  "peerDependencies": {
    "tyranid": "^0.1.45"
  },
  "devDependencies": {
    "@types/js-yaml": "3.5.29",
    "@types/lodash": "4.14.62",
    "@types/node": "6.0.68",
    "ava": "0.19.1",
